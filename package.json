{
  "name": "hello-prisma",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch",
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/src/server.js",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "seed": "npx prisma db seed",
    "start": "node dist/src/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.17",
    "concurrently": "^7.6.0",
    "kill-port": "^2.0.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^9.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "description": ""
}
